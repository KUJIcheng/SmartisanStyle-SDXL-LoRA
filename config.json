{
  "sdxl": true,
  "LoRA_type": "Standard",

  "network_dim": 64,
  "network_alpha": 64,
  "train_blocks": "all",

  "train_data_dir": "YOUR_DATASET_PATH",
  "caption_extension": ".txt",
  "train_on_input": true,
  "shuffle_caption": false,

  "enable_bucket": true,
  "min_bucket_reso": 256,
  "max_bucket_reso": 1024,
  "bucket_reso_steps": 64,
  "max_resolution": "1024,1024",

  "optimizer": "AdamW",
  "learning_rate": 0.0001,
  "unet_lr": 0.0001,
  "train_batch_size": 64,
  "loss_type": "l2",
  "lr_scheduler": "cosine",
  "lr_warmup": 10,
  "max_grad_norm": 1,

  "mixed_precision": "no",
  "cache_latents": true,
  "xformers": "xformers",

  "epoch": 10,
  "save_every_n_epochs": 1,
  "save_model_as": "safetensors",
  "save_precision": "fp16"
}
